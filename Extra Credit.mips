    .data
prompt_L: .asciiz "ENTER L (L > 20):\n"
prompt_M: .asciiz "ENTER M (M > 20):\n"
prompt_N: .asciiz "ENTER N (N > 20):\n"
error: .asciiz "Illegal Number!\n"

.text 
.global main
main:
    j  loop                # jump to input_loop to start the process

loop:
    # Prompt for L
    li $v0, 4                  # syscall code for printing string
    la $a0, prompt_L           # load address of prompt string
    syscall                    # print prompt
